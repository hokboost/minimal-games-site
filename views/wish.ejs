            <div class="guarantee-notice">
                <h4>ğŸ¯ ç¥ˆæ„¿è§„åˆ™</h4>
                <p>â€¢ ç³»ç»Ÿä¿è¯å…¨æœç»¼åˆä¸­å¥–ç‡ä¸º 1.35%</p>
                <p>â€¢ æ¦‚ç‡ä¼šæ ¹æ®å…¨æœç»Ÿè®¡åŠ¨æ€è°ƒæ•´</p>
                <p>â€¢ è¿ç»­147æ¬¡æœªä¸­å¥–åï¼Œç¬¬148æ¬¡å¿…å®šæˆåŠŸ</p>
                <p>â€¢ æ¯æ¬¡æˆåŠŸåé‡ç½®ä¸ªäººè¿›åº¦è®¡æ•°</p>
            </div>
            
            <!-- å…è´£å£°æ˜ -->
            <div style="
                max-width: 760px;
                margin: 2rem auto 0;
                padding: 1rem;
                background: rgba(255, 255, 255, 0.05);
                border-radius: 8px;
                font-size: 0.85rem;
                color: #ccc;
                line-height: 1.5;
                text-align: center;
            ">
                âš ï¸ <strong>å…è´£å£°æ˜ï¼š</strong>æœ¬è®¸æ„¿åŠŸèƒ½ä»…ä¾›å¨±ä¹æ¶ˆé£ï¼Œç¥ˆæ„¿ç»“æœä¸ºéšæœºå¨±ä¹å†…å®¹ï¼Œä¸æ¶‰åŠä»»ä½•è¿·ä¿¡æˆ–è¶…è‡ªç„¶å› ç´ ã€‚å¹³å°å†…æ‰€æœ‰ç§¯åˆ†ã€è¿›åº¦å‡ä¸ºè™šæ‹Ÿå¨±ä¹é“å…·ï¼Œæ— æ³•å…‘æ¢çœŸå®è´§å¸æˆ–æœ‰ä»·ç‰©å“ã€‚å‚ä¸å®Œå…¨å…è´¹ï¼Œç¬¦åˆç›¸å…³æ³•å¾‹æ³•è§„ã€‚ä¸¥ç¦æœªæ»¡18å‘¨å²ç”¨æˆ·ä½¿ç”¨ï¼Œè¯·ç†æ€§å¨±ä¹ã€‚
            </div>
        </div>
    </div>

    <!-- é£˜å±å®¹å™¨ -->
    <div id="danmaku-container" class="danmaku-container"></div>

    <!-- å…¨å±å¼¹çª— -->
    <div id="fullscreenModal" class="fullscreen-modal">
        <div id="modalContent" class="modal-content"></div>
    </div>

    <script>
        const username = "<%= username %>";
        const csrfToken = "<%= csrfToken %>";
        
        // ä»localStorageè·å–ç¥ˆæ„¿æ•°æ®
        let wishData = JSON.parse(localStorage.getItem('wishData_' + username) || '{"total": 0, "success": 0, "current": 0}');
        
        // å…¨æœç»¼åˆç‡åˆå§‹å€¼
        let lastGlobalRate = 1.35;
        
        function updateDisplay(serverData = null) {
            document.getElementById('totalWishes').textContent = wishData.total;
            document.getElementById('successCount').textContent = wishData.success;
            document.getElementById('currentProgress').textContent = `${wishData.current} / 148`;
            
            // æ›´æ–°ä¸ªäººä¸­å¥–ç‡
            const personalRate = wishData.total > 0 ? (wishData.success / wishData.total * 100) : 0;
            document.getElementById('personalRate').textContent = personalRate.toFixed(2) + '%';
            
            // æ›´æ–°å…¨æœç»¼åˆç‡ï¼ˆå¦‚æœæœ‰æœåŠ¡å™¨æ•°æ®ï¼‰
            if (serverData && serverData.globalRate !== undefined) {
                lastGlobalRate = serverData.globalRate;
                const globalRate = (serverData.globalRate * 100).toFixed(2);
                document.getElementById('globalRate').textContent = globalRate + '%';
            } else {
                document.getElementById('globalRate').textContent = (lastGlobalRate * 100).toFixed(2) + '%';
            }
            
            const progressPercent = (wishData.current / 148) * 100;
            document.getElementById('progressBar').style.width = progressPercent + '%';
            document.getElementById('progressText').textContent = `${wishData.current} / 148`;
        }
        
        function saveWishData() {
            localStorage.setItem('wishData_' + username, JSON.stringify(wishData));
        }
        

        function showModal(isSuccess, reward = null, rewardValue = 0, isGuaranteed = false) {
            const modal = document.getElementById('fullscreenModal');
            const content = document.getElementById('modalContent');
            
            if (isSuccess) {
                content.innerHTML = `
                    <div>ğŸ‰ ç¥ˆæ„¿æˆåŠŸï¼</div>
                    <div style="font-size: 2rem; margin: 15px 0;">ğŸ§œâ€â™€ï¸ ${reward || 'æ·±æµ·æ­Œå§¬'}</div>
                    <div style="font-size: 1.5rem; color: #f39c12;">ä»·å€¼: ${rewardValue || 30000} ç”µå¸</div>
                    ${isGuaranteed ? '<div style="font-size: 1rem; color: #e74c3c; margin-top: 10px;">ä¿åº•å‡ºè´§</div>' : ''}
                `;
                content.className = 'modal-content modal-success';
            } else {
                content.textContent = 'ğŸ˜¢ ç¥ˆæ„¿å¤±è´¥ï¼Œå†æ¥å†å‰ï¼';
                content.className = 'modal-content modal-failure';
            }
            
            modal.style.display = 'flex';
            
            // 3ç§’åè‡ªåŠ¨å…³é—­ï¼Œç‚¹å‡»å¯æå‰å…³é—­
            setTimeout(() => {
                if (modal.style.display === 'flex') {
                    modal.style.display = 'none';
                }
            }, 3000);
        }
        
        async function makeWish() {
            const button = document.getElementById('wishButton');
            button.disabled = true;
            button.textContent = 'ç¥ˆæ„¿ä¸­...';
            
            try {
                const response = await fetch('/api/wish/play', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'X-CSRF-Token': csrfToken
                    }
                });
                
                const result = await response.json();
                
                if (result.success) {
                    // æ›´æ–°æœ¬åœ°è¿›åº¦æ•°æ®
                    wishProgress = result.progress;
                    wishProgress.total_spent = result.progress.total_wishes * 500; // è®¡ç®—æ€»èŠ±è´¹
                    
                    if (typeof result.newBalance === 'number') {
                        document.getElementById('current-balance').textContent = result.newBalance;
                    }
                    
                    // æ˜¾ç¤ºç»“æœ
                    if (result.wishSuccess) {
                        showModal(true, result.reward, result.rewardValue, result.isGuaranteed);
                    } else {
                        showModal(false);
                    }
                    
                    updateDisplayNew();
                } else {
                    alert('ç¥ˆæ„¿å¤±è´¥ï¼š' + result.message);
                }
            } catch (error) {
                console.error('Error:', error);
                alert('ç½‘ç»œé”™è¯¯ï¼Œè¯·é‡è¯•');
            } finally {
                button.disabled = false;
                button.textContent = 'ğŸ™ è®¸æ„¿ç¥ˆç¥·';
            }
        }
        
        // é¡µé¢åŠ è½½æ—¶è·å–ç¥ˆæ„¿è¿›åº¦
        async function loadWishProgress() {
            try {
                const response = await fetch('/api/wish/progress');
                const result = await response.json();
                
                if (result.success) {
                    wishProgress = result.progress;
                    updateDisplayNew();
                }
            } catch (error) {
                console.error('åŠ è½½ç¥ˆæ„¿è¿›åº¦å¤±è´¥:', error);
                updateDisplay(); // fallbackåˆ°åŸæ¥çš„æ˜¾ç¤º
            }
        }

        function updateDisplayNew() {
            const progress = (wishProgress.consecutive_fails / 148) * 100;
            document.getElementById('progressBar').style.width = Math.min(progress, 100) + '%';
            document.getElementById('progressText').textContent = 
                `${wishProgress.consecutive_fails} / 148 (ä¿åº•)`;
            
            document.getElementById('totalWishes').textContent = wishProgress.total_wishes;
            document.getElementById('successCount').textContent = 
                Math.floor(wishProgress.total_rewards_value / 30000); // æ ¹æ®è·å¾—å¥–åŠ±è®¡ç®—æˆåŠŸæ¬¡æ•°
            
            const failCount = wishProgress.total_wishes - Math.floor(wishProgress.total_rewards_value / 30000);
            if (document.getElementById('failCount')) {
                document.getElementById('failCount').textContent = failCount;
            }
            
            const successRate = wishProgress.total_wishes > 0 ? 
                (Math.floor(wishProgress.total_rewards_value / 30000) / wishProgress.total_wishes * 100).toFixed(1) : '0.0';
            if (document.getElementById('successRate')) {
                document.getElementById('successRate').textContent = successRate + '%';
            }
            
            // å¦‚æœå³å°†ä¿åº•ï¼Œæ”¹å˜è¿›åº¦æ¡é¢œè‰²
            if (wishProgress.consecutive_fails >= 147) {
                document.getElementById('progressBar').style.background = 
                    'linear-gradient(45deg, #f39c12, #e67e22)'; // é‡‘è‰²ï¼Œè¡¨ç¤ºå³å°†ä¿åº•
            } else {
                document.getElementById('progressBar').style.background = 
                    'linear-gradient(45deg, #ff6b6b, #ee5a24)';
            }
        }
        
        // åˆå§‹åŒ–æ˜¾ç¤º
        loadWishProgress();
        
        // é£˜å±ç®¡ç†å™¨
        class DanmakuManager {
            constructor() {
                this.container = document.getElementById('danmaku-container');
                this.usedLanes = new Set(); // é¿å…é‡å 
                this.maxLanes = Math.floor((window.innerHeight - 200) / 50);
            }
            
            addMessage(data) {
                const message = document.createElement('div');
                message.className = 'danmaku-message';
                message.textContent = data.content;
                
                // éšæœºé€‰æ‹©ä¸é‡å çš„è½¨é“
                const lane = this.getAvailableLane();
                message.style.top = `${lane * 50 + 100}px`;
                
                this.container.appendChild(message);
                
                // 12ç§’åç§»é™¤
                setTimeout(() => {
                    if (message.parentNode) {
                        message.parentNode.removeChild(message);
                    }
                    this.usedLanes.delete(lane);
                }, 12000);
                
                this.usedLanes.add(lane);
            }
            
            getAvailableLane() {
                for (let i = 0; i < this.maxLanes; i++) {
                    if (!this.usedLanes.has(i)) {
                        return i;
                    }
                }
                // å¦‚æœæ²¡æœ‰ç©ºé—²è½¨é“ï¼Œéšæœºé€‰æ‹©ä¸€ä¸ª
                return Math.floor(Math.random() * this.maxLanes);
            }
        }
        
        const danmakuManager = new DanmakuManager();
        
        // WebSocketè¿æ¥
        let socket;
        
        function initSocket() {
            console.log('åˆå§‹åŒ–Socketè¿æ¥...');
            socket = io();
            
            socket.on('connect', () => {
                console.log('Socketè¿æ¥æˆåŠŸï¼');
            });
            
            socket.on('disconnect', () => {
                console.log('Socketè¿æ¥æ–­å¼€');
            });
            
            // ç›‘å¬æ–°é£˜å±æ¶ˆæ¯
            socket.on('new_danmaku', (data) => {
                console.log('æ”¶åˆ°é£˜å±æ¶ˆæ¯:', data);
                danmakuManager.addMessage(data);
            });
            
            // æ¥æ”¶å†å²æ¶ˆæ¯
            socket.on('recent_messages', (messages) => {
                console.log('æ”¶åˆ°å†å²æ¶ˆæ¯:', messages);
                // æ˜¾ç¤ºæœ€è¿‘çš„3æ¡æˆåŠŸæ¶ˆæ¯
                messages.slice(0, 3).forEach((msg, index) => {
                    setTimeout(() => {
                        danmakuManager.addMessage(msg);
                    }, index * 1000);
                });
            });
        }
        
        // é¡µé¢åŠ è½½å®Œæˆååˆå§‹åŒ–Socket
        document.addEventListener('DOMContentLoaded', function() {
            initSocket();
        });
        
        // åˆå§‹åŒ–æ˜¾ç¤º
        updateDisplay();
        
        // ç¥ˆæ„¿åæ¬¡åŠŸèƒ½
        async function makeWishTen() {
            await batchWish(10, 'wishTenButton', 'ğŸ™ ç¥ˆæ„¿åæ¬¡');
        }

        // æ‰¹é‡ç¥ˆæ„¿é€šç”¨å‡½æ•°
        async function batchWish(count, buttonId, buttonText) {
            const button = document.getElementById(buttonId);
            const allButtons = [
                document.getElementById('wishButton'),
                document.getElementById('wishTenButton')
            ];
            
            // ç¦ç”¨æ‰€æœ‰æŒ‰é’®
            allButtons.forEach(btn => btn.disabled = true);
            button.textContent = 'ç¥ˆæ„¿ä¸­...';
            
            try {
                // å‘é€æ‰¹é‡è¯·æ±‚
                const response = await fetch('/api/wish-batch', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'X-CSRF-Token': csrfToken
                    },
                    body: JSON.stringify({ 
                        batchCount: count
                    })
                });
                
                const result = await response.json();
                
                if (result.success) {
                    if (result.progress) {
                        wishProgress = result.progress;
                        updateDisplayNew();
                    }
                    
                    if (typeof result.newBalance === 'number') {
                        document.getElementById('current-balance').textContent = result.newBalance;
                    }
                    
                    // æ˜¾ç¤ºç»“æœ
                    const modal = document.getElementById('fullscreenModal');
                    const content = document.getElementById('modalContent');
                    const rate = ((result.successCount / count) * 100).toFixed(2);
                    content.textContent = `${count}æ¬¡ç¥ˆæ„¿å®Œæˆï¼æˆåŠŸ${result.successCount}æ¬¡ (${rate}%)`;
                    content.className = result.successCount > 0 ? 'modal-content modal-success' : 'modal-content modal-failure';
                    modal.style.display = 'flex';
                    
                    setTimeout(() => {
                        modal.style.display = 'none';
                    }, 4000);
                } else {
                    alert('æ‰¹é‡ç¥ˆæ„¿å¤±è´¥ï¼š' + result.message);
