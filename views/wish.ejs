        .balance-info h3 {
            color: #ffeb3b;
            margin: 0;
        }

        .wish-progress {
            margin: 30px 0;
            background: rgba(255, 255, 255, 0.1);
            border-radius: 10px;
            overflow: hidden;
            height: 40px;
            position: relative;
        }

        .progress-bar {
            height: 100%;
