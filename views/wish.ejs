        }

        .nav-links {
            display: flex;
            gap: 1rem;
            align-items: center;
            color: #ccc;
        }

        .nav-links a {
            color: #fff;
            text-decoration: none;
            padding: 0.5rem 1rem;
            border-radius: 8px;
            background: rgba(255, 255, 255, 0.1);
            transition: all 0.3s ease;
        }

        .nav-links a:hover {
            background: rgba(0, 200, 83, 0.2);
            transform: translateY(-2px);
        }

        .wish-container {
            text-align: center;
            max-width: 800px;
            margin: 0 auto;
        }

        .balance-wish-section {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 12px;
            padding: 2rem;
            margin-bottom: 2rem;
        }

        .balance-info {
            text-align: center;
